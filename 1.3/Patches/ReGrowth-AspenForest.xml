<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>ReGrowth: Aspen Forests</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName="RG-AF_AspenForest"]/wildPlants</xpath>
					<value>
						<RG-BF_Plant_BorealFern>1.0</RG-BF_Plant_BorealFern>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>