<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>ReGrowth: Aspen</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName="RG_AspenForest"]/wildPlants</xpath>
					<value>
						<RG_Plant_BorealFern>0.2</RG_Plant_BorealFern>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>